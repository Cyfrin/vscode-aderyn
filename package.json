{
  "name": "aderyn",
  "displayName": "Aderyn",
  "description": "Support for the Solidity programming language",
  "version": "1.0.0",
  "publisher": "cyfrin",
  "repository": {
    "type": "git",
    "url": "https://github.com/cyfrin/vscode-aderyn"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "icon": "images/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "aderyn.restartServer",
        "title": "Restart Aderyn Server",
        "category": "Aderyn"
      }
    ]
  },
  "activationEvents": [
    "onLanguage"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode development",
    "compile-watch": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "vscode:package": "vsce package",
    "prettier:check": "prettier --list-different 'src/**/*.{js,jsx,ts,tsx}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "@vscode/vsce": "^2.29.0",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^7.32.0",
    "glob": "^7.1.6",
    "globals": "^15.9.0",
    "mocha": "^10.1.0",
    "prettier": "^3.3.3",
    "ts-loader": "^8.0.13",
    "typescript": "^4.1.2",
    "typescript-eslint": "^8.5.0",
    "vscode-test": "^1.4.1",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "follow-redirects": "^1.15.9",
    "node-uname": "^3.1.1",
    "tar": "^7.4.3",
    "vscode-languageclient": "^8.1.0"
  }
}
